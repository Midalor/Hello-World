n = int(input())
if n > 0:
    if n % 2 == 0:
        print('Положительное четное число')
    else:
        print('Положительное нечетное число')
elif n < 0:
    if n % 2 == 0:
        print('Отрицательное четное число')
    else:
        print('Отрицательное нечетное число')
else:
    print('Нулевое число')
